{
  "name": "rollup",
  "version": "1.0.0",
  "description": "rollup demo",
  "main": "app.js",
  "scripts": {
    "build": "rollup -c",
    "build:amd": "rollup app.js -f amd -o ./dist/dist.amd.js",
    "build:cjs": "rollup app.js -f cjs -o ./dist/dist.cjs.js",
    "build:es": "rollup app.js -f es -o ./dist/dist.es.js",
    "build:iife": "rollup app.js -f iife -o ./dist/dist.iife.js",
    "build:umd": "rollup app.js -f umd -o ./dist/dist.umd.js",
    "build:all": "npm run build:amd && npm run build:cjs && npm run build:es && npm run build:iife && npm run build:umd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caichunbao/rollup.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/caichunbao/rollup/issues"
  },
  "homepage": "https://github.com/caichunbao/rollup#readme",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3"
  }
}
